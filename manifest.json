{
	"name": "PageRank Fast",
	"version": "2.2",
	"manifest_version": 2,
	"description": "Fast and simple PageRank extension for Google Chrome.",
	"icons": {
		"128": "images/icon128.png",
		"48": "images/icon48.png"
	},
	"browser_action": {
		"default_icon": {
			"19": "images/0.png"
		}
	},
	"background": {
		"page": "background.html"
	},
	"permissions": ["tabs", "http://*/"]
}